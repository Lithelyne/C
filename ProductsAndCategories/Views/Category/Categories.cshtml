@model List<Category>
@{
  Category category = new Category();
}
<div class="container">
  <div class="row">
    <div class="col-md-6" style="border: 4px solid;">@await Html.PartialAsync("_NewCategory", category)</div>
    <div class="col-md-6" style="border: 4px solid;">@await Html.PartialAsync("_AllCategories")</div>
  </div>
</div>