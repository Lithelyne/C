@model List<Product>

<div class="text-center mt-3" style="border: 4px solid;">
  <div class="container mt-3">
    <h1>All Products</h1>
    <table class="table">
      <tbody>
        @foreach (Product? product in Model)
        {
          <tr>
            <td><a href="products/@product.ProductId">@product.Name</a></td>
          </tr>
        }
      </tbody>
    </table>
  </div>
</div>
